<html>
  <head>
    <Title> ToDo List</Title>
    <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
    <script src="/assets/todo-list.js"></script>
    <link href="/assets/styles.css" rel="stylesheet" text="text/css" />
  </head>
  <body>
      <div id="todo-table">
        <form>
          <input type="text" name="item" palceholder="Add New Item..." required />
          <button type="submit">Add Item</button>
        </form>
      </div>
      <ul>
        <% for(var i = 0; i < todos.length; i++){ %>
          <li><%= todos[i].item %></li>
          <% } %>
      </ul>
  </body>

</html>
